<div class="container">
    <div class="card bg-aqua">
        <div class="row card-header text-white">
            <h3 class="col-md-10">Proyectos Concluidos</h3>
        </div>
        <table class="table table-hover table-sm table-dark card-body">
            <thead>
                <tr>
                <th scope="col">ID</th>
                <th scope="col">Nombre</th>
                <th scope="col"></th>
                </tr>
            </thead>
            <tbody>
            <% @proyectos.each do |proyecto|%>
                    <tr>
                    <td><%=proyecto.id%></td>
                    <td><%=proyecto.nombre%></td>
                    <td><%= link_to 'Ver Detalles', dashboard_index_path(:proyecto_id => proyecto.proyecto.id)%></td>
                    </tr>
            <% end %>
            </tbody>
        </table>
    </div>
</div>